{
  "name": "sivananda-housekeeping",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/danthareja/sivananda-housekeeping.git",
  "author": "Dan Thareja <danthareja@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "16.13.x",
    "npm": "8.1.x"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "NODE_ENV=development nodemon index.js --ignore client --ignore bin",
    "dev:client": "cd client && npm start",
    "postinstall": "cd client && npm install && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.2.1",
    "apollo-server-caching": "^0.2.1",
    "apollo-server-express": "^2.1.0",
    "axios": "^0.18.0",
    "cachegoose": "^7.1.0",
    "csv-parse": "^3.1.3",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "jsonwebtoken": "^8.3.0",
    "jwks-rsa": "^1.3.0",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.7",
    "moment": "^2.22.2",
    "mongoose": "^5.3.7"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "dotenv": "^6.0.0",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "nodemon": "^1.18.4",
    "prettier": "1.14.3"
  }
}
