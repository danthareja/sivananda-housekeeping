type Room {
  id: ID! @unique # Prisma manages this field
  retreatGuruId: Int @unique # We use this field interally
  name: String! # from seed
  lodgingId: Int! # from seed
  lodgingName: String! # from seed
  location: String! # from seed
  cleaningTime: Int! # from seed
  cartCost: Int! # from seed
  dirty: Boolean!
  givenKey: Boolean!
  cleanedAt: DateTime
  housekeeper: String
  order: Int
  comments: [String!]!
}
