type Room {
  id: ID! @unique # Prisma manages this field

  retreatGuruId: Int @unique # from seed
  name: String! # from seed
  lodgingId: Int! # from seed
  lodgingName: String! # from seed
  location: String! # from seed
  cleaningTime: Int! # from seed
  cleaningCartCost: Int! # from seed

  cleaned: Boolean!
  cleanedAt: DateTime
  givenKey: Boolean!
  givenKeyAt: DateTime

  housekeeper: String
  priority: Int
  comments: [String!]!
}
